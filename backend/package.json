{
  "name": "backend-my-estimate-me",
  "version": "1.0.0",
  "description": "Estimates made easy.",
  "main": "./dist/server.js",
  "engines": {
    "node": "22.12.0",
    "npm": "10.9.0"
  },
  "scripts": {
    "dev": "tsx watch ./src/server.ts",
    "build": "tsup ./src --out-dir ./build",
    "start": "node build/server.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:seed": "tsx ./src/prisma/seeds/index.ts",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "test": "vitest run ",
    "test:watch": "vitest"
  },
  "prisma": {
    "schema": "./src/prisma/schema"
  },
  "author": "Danillo Estrela",
  "license": "ISC",
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@fastify/auth": "4.6.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.0.3",
    "@fastify/oauth2": "7.9.0",
    "@fastify/rate-limit": "10.0.1",
    "@fastify/type-provider-typebox": "4.1.0",
    "@fastify/websocket": "11.0.1",
    "@sinclair/typebox": "0.33.9",
    "axios": "1.7.7",
    "bcryptjs": "2.4.3",
    "date-fns": "3.6.0",
    "dotenv": "16.0.3",
    "fastify": "^5.2.1",
    "fastify-i18next": "0.5.1",
    "fastify-plugin": "4.5.1",
    "i18next": "23.15.1",
    "i18next-fs-backend": "2.3.2",
    "jsonwebtoken": "9.0.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "9.10.0",
    "@prisma/client": "^6.2.1",
    "@types/bcryptjs": "2.4.6",
    "@types/i18next": "13.0.0",
    "@types/i18next-fs-backend": "1.1.5",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "22.5.4",
    "@types/ws": "^8.5.12",
    "eslint": "^9.17.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "15.9.0",
    "prettier": "3.3.3",
    "prisma": "^6.2.1",
    "tsconfig-paths": "4.2.0",
    "tsup": "^2.0.3",
    "tsx": "^4.19.3",
    "typescript": "^5.1.0",
    "typescript-eslint": "^8.4.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^0.0.28",
    "vitest-tsconfig-paths": "^3.4.1"
  }
}
